{
  "extends": [
    "tslint:recommended",
    "tslint-clean-code"
  ],
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "indent": [true, "tabs", 2],
    "member-access": false,
    "quotemark": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "no-console": false
  }
}
